FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application icoDyMFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         1;

deltaT          0.0005;

// writeControl    timeStep;
writeControl runTime;

writeInterval 0.01;

purgeWrite      0;

writeFormat     binary;

writePrecision  6;

writeCompression compressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

adjustTimeStep  no;

maxCo           0.2;

functions
{
    immersedBoundaryForce
    {
        type               immersedBoundaryForce;
        functionObjectLibs ("libimmersedBoundaryForceFunctionObject.so");
        Aref 1;
        Uref 1;
    }
}


